{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "cli": {
    "packageManager": "npm",
    "analytics": "041c515d-12d0-41db-aaf0-3a9b9077397e"
  },
  "newProjectRoot": "projects",
  "projects": {
    "JuegoAdivina": {
      "projectType": "application",
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular/build:application",
          "options": {
            "browser": "src/main.ts",
            "tsConfig": "tsconfig.app.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "public"
              }
            ],
            "styles": ["src/styles.css"],
            "outputPath": "dist/JuegoAdivina",
            "baseHref": "/JuegoAdivina/",
            "outputMode": "static"    
          },
          "configurations": {
            "production": {
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular/build:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "JuegoAdivina:build:production"
            },
            "development": {
              "buildTarget": "JuegoAdivina:build:development"
            }
          },
          "defaultConfiguration": "development"
        }
      }
    }
  }
}
